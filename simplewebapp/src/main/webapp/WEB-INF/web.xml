<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>MyFirstServlet</servlet-name>
  	<display-name>MyFirstServlet</display-name>
  	<description></description>
  	<servlet-class>in.synxt.servlets.MyFirstServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EmployeeServlet</servlet-name>
  	<display-name>EmployeeServlet</display-name>
  	<description></description>
  	<servlet-class>in.synxt.servlets.EmployeeServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EmployeeSearcher</servlet-name>
  	<display-name>EmployeeSearcher</display-name>
  	<description></description>
  	<servlet-class>in.synxt.servlets.EmployeeSearcher</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>MyFirstServlet</servlet-name>
  	<url-pattern>/myfirst</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EmployeeServlet</servlet-name>
  	<url-pattern>/emps</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EmployeeSearcher</servlet-name>
  	<url-pattern>/search</url-pattern>
  </servlet-mapping>
</web-app>
